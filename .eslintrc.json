{
    "extends": "airbnb-base",
    "env": {
        "es6": true,
        "browser": true,
        "jest": true
    },
    "rules": {
        "class-methods-use-this": 0,
        "no-param-reassign": 0,
        "no-new": 0,
        "guard-for-in": 0,
        "no-shadow": 0,
        "no-restricted-syntax": [
            "error",
            "LabeledStatement",
            "WithStatement"
        ]
    }
}